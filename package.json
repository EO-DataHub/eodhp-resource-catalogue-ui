{
  "name": "eodhp-resource-catalogue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.12.2"
  },
  "homepage": "/static-apps/resource-catalogue/main/",
  "scripts": {
    "compile": "tsc --noEmit",
    "dev": "npm run compile && vite",
    "dev:msw": "tsc --noEmit && VITE_MSW_ENABLED=enabled vite",
    "build": "npm run compile && npm run lint && npm run lint:css && vite build",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:rules": "eslint --print-config rules > eslint-rules.json",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:css:fix": "stylelint '**/*.{css,scss}' --fix",
    "lint:css:rules": "stylelint --print-config rules > stylelint-rules.json",
    "postinstall": "npx husky install .husky",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@turf/turf": "^7.1.0",
    "axios": "^1.7.4",
    "date-fns": "^3.6.0",
    "geojson": "^0.5.0",
    "leaflet": "^1.9.4",
    "ol-stac": "^1.0.0-beta.10",
    "optionator": "^0.9.4",
    "react": "^18.3.1",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.2.1",
    "react-responsive-pagination": "^2.6.0",
    "react-router-dom": "^6.21.1",
    "react-select": "^5.8.0",
    "react-tooltip": "^5.28.0",
    "react-use": "^17.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.9.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@tpzdsp/eslint-config-dsp": "^1.0.1",
    "@types/geojson": "^7946.0.14",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^24.1.1",
    "msw": "^2.3.5",
    "ol": "^10.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.69.7",
    "stylelint": "^16.8.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^2.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
